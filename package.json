{
  "name": "efx-forms",
  "version": "0.0.9",
  "description": "Effector JS Forms",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./react": "./dist/react.js",
    "./validators": "./dist/validators.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      ".": ["dist/index.d.ts"],
      "react": ["dist/react.d.ts"],
      "validators": ["dist/validators.d.ts"]
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "publish-lib": "npm run build && npm publish",
    "publish-beta": "npm run build && npm publish --tag beta",
    "publish-dryrun": "npm run build && npm publish --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darianstlex/efx-forms.git"
  },
  "keywords": [
    "typescript",
    "effector",
    "forms",
    "library"
